<div class="profile">
    <div class="col s12 card--profile widthNone">

        <div class="row">
            <div class="col s2"></div>

            <div class="col s8">
                <div class="col s12 profileCard--img">
                    <div>
                        <img src="{{image}}" alt="profile" class="circle responsive-img materialboxed"> <!-- notice the "circle" class -->
                    </div>
                </div>
                <div class="col s12 center-align">
                    <p><a href="/seller/profile/{{id}}/edit">Editar</a></p>
                </div>
                <div class="col s12 card-panel z-depth-1 card--profile">
                    <div class=" col s12 profile--data no-divider">
                        <div class="col s12 data--left">
                            <p class="col s6 data--right">Nombre o razón social: </p> <p class="col s6 data--left">{{name}}</p>
                            <p class="col s6 data--right">CUIT: </p> <p class="col s6 data--left">{{cuit}}</p>
                            <p class="col s6 data--right">Teléfono: </p> <p class="col s6 data--left">{{phone}}</p>
                        </div>
                    </div>
                </div>

                <div class="col s12 card-panel z-depth-1 card--profile">
                    <div class="col s12 profile--data no-divider">
                        <div class="col s6">
                            <p class="col s12 data--right">Direcciones:</p>
                        </div>
                        <div class="col s6 data--left">
                            {{#addresses}}
                                <p>{{address}}</p>
                                <p>{{wrapper}} </p>
                            {{/addresses}}
                        </div>
                    </div>
                </div>

                <div class="col s12 card-panel z-depth-1  card--profile">
                    <div class=" col s12 profile--data no-divider">
                        <div class="col s12">
                            <div class="col s6">
                                <p class="data--right">Mail: </p>
                                <p class="data--right">Clave: </p>
                            </div>
                            <div class="col s6">
                                <p class="data--left">{{mail}}</p>
                                <p class="data--left">*********</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col s2"></div>
        </div>

    </div>
</div>
